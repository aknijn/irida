<?xml version="1.0" encoding="UTF-8"?>

<iridaWorkflow>
	<id>9d8dd9c3-56cb-4423-8ce8-8972f7a91ee3</id>
	<name>ARIESExport</name>
	<version>1.0</version>
	<analysisType>ARIES_EXPORT</analysisType>
	<inputs>
		<sequenceReadsPaired>input_collection</sequenceReadsPaired>
		<requiresSingleSample>false</requiresSingleSample>
	</inputs>
	<parameters>
		<parameter name="phante_species" defaultValue="Escherichia coli">
			<toolParameter toolId="phante" parameterName="species" />
		</parameter>
		<parameter name="phante_user" defaultValue="user">
			<toolParameter toolId="phante" parameterName="user" />
		</parameter>
	</parameters>
	<outputs>
        <output name="phante_csv" fileName="iridaaries-export.csv"/>
	</outputs>
	<toolRepositories>
		<repository>
		  <name>collapse_collections</name>
		  <owner>nml</owner>
		  <url>https://irida.corefacility.ca/galaxy-shed</url>
		  <revision>7e5a39b5dad3</revision>
		</repository>
	</toolRepositories>
</iridaWorkflow>
