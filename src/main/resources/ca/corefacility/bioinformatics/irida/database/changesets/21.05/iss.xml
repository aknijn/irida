<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<changeSet id="iss" author="knijn">

		<addColumn tableName="sample">
			<column name="patientAge" type="varchar(7)" />
		</addColumn>

		<addColumn tableName="sample_AUD">
			<column name="patientAge" type="varchar(7)" />
		</addColumn>

		<addColumn tableName="sample">
			<column name="patientVaccinationNumber" type="varchar(1)" />
		</addColumn>

		<addColumn tableName="sample_AUD">
			<column name="patientVaccinationNumber" type="varchar(1)" />
		</addColumn>

		<addColumn tableName="sample">
			<column name="patientVaccinationDate" type="date" />
		</addColumn>

		<addColumn tableName="sample_AUD">
			<column name="patientVaccinationDate" type="date" />
		</addColumn>

	</changeSet>

</databaseChangeLog>
